  <div class="d-flex flex-row flex-wrap">
    <% influencers.each do |influencer| %>
      <div class="card influencer-to-add" id="influencer-<%= influencer.id %>">
        <h3 class="title"><%= influencer.name %></h3>
          <div>
            <% if influencer.photo.attached?  %>
              <%= cl_image_tag influencer.photo.key, class:"photocard" %>>
            <% end %>

            <div class="bar">
              <h4 class="title">Communaut√©</h4>
              <div class="emptybar"></div>
              <div class="filledbar"></div>
           </div>
            <div class="bar2">
              <h4 class="title">Localisation</h4>
              <div class="emptybar"></div>
              <div class="filledbar2"></div>
           </div>
                <div class="bar3">
              <h4 class="title">Age</h4>
              <div class="emptybar"></div>
              <div class="filledbar3"></div>
           </div>
                <div class="bar4">
              <h4 class="title">Engagement Rate</h4>
              <div class="emptybar"></div>
              <div class="filledbar4"></div>
           </div>
        </div>

        <div class="circle">
          <circle class="stroke" cx="60" cy="60" r="50"/>
        </div>
      </div>
    <% end %>
  </div>

